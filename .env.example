# AI Resume Screener - Example environment variables
# Copy this file to `.env` and replace YOUR_KEY_VALUE with real values.

# n8n core
N8N_PORT=5678
N8N_HOST=0.0.0.0
N8N_PATH=/data
GENERIC_TIMEZONE=Asia/Kolkata

# n8n basic auth (recommended)
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=YOUR_KEY_VALUE

# Encryption key for n8n credentials (must be 32 chars)
N8N_ENCRYPTION_KEY=YOUR_KEY_VALUE

# Optional public webhook/base URL (for tunnels or hosted)
WEBHOOK_TUNNEL_URL=YOUR_KEY_VALUE

# Integrations
AIRTABLE_API_KEY=YOUR_KEY_VALUE
AIRTABLE_BASE_ID=YOUR_KEY_VALUE
GOOGLE_API_KEY=YOUR_KEY_VALUE
